{
  "manifest_version": 3,
  "name": "Web Vitals Analyzer",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
  "action" : {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions" : ["scripting","activeTab","storage"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}